<h1>Log In</h1>
<h4>Kindly Login with your username</h4>
<%= form_with(url:login_path, scope: :session, local: true) do |form| %>
  
  <div class="form-group">
  <%= form.text_field :username, placeholder: "Enter your Username" %>
  </div>
  <%= form.submit value = "Login", class: "btn btn-primary" %>
<% end %>

